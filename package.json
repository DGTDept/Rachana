{
  "name": "@govtechsg/sgds",
  "version": "2.0.0-alpha.4",
  "description": "Design System unites Singapore Government digital services around a common visual language and user experience",
  "repository": {
    "type": "git",
    "url": "https://github.com/govtechsg/sgds.git"
  },
  "license": "MIT",
  "author": "Tey Liang Tai",
  "main": "css/sgds.css",
  "files": [
    "sass",
    "css"
  ],
  "scripts": {
    "build": "npm run packagedownloads && npm run build:site && bundle exec jekyll build",
    "build:lib": "rm -rf ./sgds/sgds.css && webpack --mode=production",
    "build:site": "npm run build:lib",
    "dev": "concurrently npm:dev:static npm:dev:jekyll",
    "dev:jekyll": "bundle exec jekyll serve",
    "dev:static": "webpack --watch",
    "packagedownloads": "./build/packagedownloads.sh",
    "prepublishOnly": "npm run build:lib",
    "start": "bundle exec jekyll serve"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "clipboard": "^2.0.10",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@govtechsg/sgds-masthead": "^0.0.9",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "flatpickr": "^4.6.9",
    "gsap": "^3.9.1",
    "shufflejs": "^6.0.0"
  }
}
