{
  "name": "@govtechsg/sgds-govtech",
  "version": "2.0.0",
  "description": "Design System unites Singapore Government digital services around a common visual language and user experience",
  "repository": {
    "type": "git",
    "url": "https://github.com/govtechsg/sgds.git"
  },
  "license": "MIT",
  "author": "Tey Liang Tai",
  "main": "sgds/sgds.css",
  "files": [
    "sgds/sass",
    "sgds/sgds.css",
    "sgds/sgds.scss"
  ],
  "scripts": {
    "build": "npm run packagedownloads && npm run build:site && bundle exec jekyll build",
    "build:lib": "rm -rf ./js ./sgds/sgds.css && webpack --mode=production",
    "build:site": "npm run build:lib",
    "dev": "concurrently npm:dev:static npm:dev:jekyll",
    "dev:jekyll": "bundle exec jekyll serve",
    "dev:static": "webpack --watch",
    "packagedownloads": "./build/packagedownloads.sh",
    "prepublishOnly": "npm run build:lib",
    "start": "bundle exec jekyll serve",
    "buildtest": "webpack --mode=production"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "axios": "^0.21.1",
    "clipboard": "^2.0.4",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^12.6.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "flatpickr": "^4.6.9"
  }
}
